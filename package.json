  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "i18n:extract": "i18next && yarn i18n:extractRuntime && yarn i18n:joinDynamicKeys",
    "i18n:upload": "ts-node -O '{\"module\": \"CommonJS\"}' ./scripts/uploadTranslations.ts",
    "i18n:extractRuntime": "ts-node -O '{\"module\": \"CommonJS\"}' ./scripts/extractRuntimeKeys.ts",
    "i18n:joinDynamicKeys": "ts-node -O '{\"module\": \"CommonJS\"}' ./scripts/joinDynamicKeys.ts",
    "test": "jest --testPathIgnorePatterns /tests",
    "test:watch": "jest --watch --testPathIgnorePatterns /tests",
    "test:coverage": "jest --coverage --testPathIgnorePatterns /tests",
    "test:integration": "jest"
  },
